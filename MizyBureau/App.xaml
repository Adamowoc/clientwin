<Application x:Class="MizyBureau.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MizyBureau"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <BitmapImage x:Key="MyImageParameter" UriSource="/MizyBureau;component/images/setting-logo.png"/>
        <BitmapImage x:Key="MyImageMizy" UriSource="images/logo.png"/>
        <BitmapImage x:Key="MyImageAnniversary" UriSource="images/anniversary-icon.png"/>
        <BitmapImage x:Key="MyImageConv" UriSource="images/conv-logo.png"/>
        <BitmapImage x:Key="MyImageFacebook" UriSource="images/facebook-logo.png"/>
        <BitmapImage x:Key="MyImageFille" UriSource="images/fille-logo.png"/>
        <BitmapImage x:Key="MyImageFrance" UriSource="images/France-icon.png"/>
        <BitmapImage x:Key="MyImageHangout" UriSource="images/hangout-logo.png"/>
        <BitmapImage x:Key="MyImageLinkedin" UriSource="images/linkedin-logo.png"/>
        <BitmapImage x:Key="MyImagePhoto" UriSource="images/profilepic.png"/>
        <BitmapImage x:Key="MyImageSMS" UriSource="images/sms-logo.png"/>
        <BitmapImage x:Key="MyImageTwitter" UriSource="images/twitter-logo.png"/>
        <!--Image x:Key="MyImage" Source="images/.png"/-->

    </Application.Resources>
</Application>
